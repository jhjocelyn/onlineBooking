<% include partials/header %>
<div class="landing">
    <nav class="navbar navbar-default nav-custom">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="/"><span class="glyphicon glyphicon-home"></span> Back To Home</a>
        </div>
      </div>
    </nav>

    <div id="login-header" class="container">
        <h1>Log In</h1>
        <h4>No account yet? <a id="signup" href="/register"> Sign Up</a></h4>
    </div>

    <div id="login-form" class="container">
        <form action="/login" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" name="username" placeholder="email">
            </div>

            <div class="form-group">
                <input class="form-control" type="password" name="password" placeholder="password">
            </div>

            <div class="form-group">
                <div class="row">
                    <div class="col-md-6"><button class="btn-sub">Login</button></div>
                    <div class="col-md-6"><a href="#" id="find-pwd">Forget password?</a></div>
                </div>
            </div>
        </form>
    </div>

</div>



<% include partials/footer %>